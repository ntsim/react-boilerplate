    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@ntsim/webpack-config-base": "^0.1.1",
    "@ntsim/webpack-config-react-typescript": "^0.1.1",
    "@types/jest": "^24.0.4",
    "@types/node": "^11.9.3",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "autoprefixer": "^9.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
